{
  "name": "@mr.mikey/create-husky",
  "version": "1.0.8",
  "description": "自动为项目工程生成husky及git hook等配置，会创建commitlint对提交信息进行规范限制（angular commit规范）、汉化commitizen配置、lint-staged仅对commit文件做lint校验。",
  "author": "JP.Chen",
  "license": "MIT",
  "main": "index.js",
  "type": "module",
  "bin": {
    "create-husky": "index.js"
  },
  "scripts": {
    "husky": "node index.js"
  },
  "files": [
    "index.js",
    "src",
    "package.json"
  ],
  "dependencies": {
    "js-beautify": "^1.14.9",
    "kolorist": "^1.8.0",
    "nanospinner": "^1.1.0",
    "prompts": "^2.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mr-Super-X/frontend-standard-engine.git",
    "directory": "packages/create-husky"
  },
  "bugs": {
    "url": "https://github.com/Mr-Super-X/frontend-standard-engine/issues"
  },
  "homepage": "https://github.com/Mr-Super-X/frontend-standard-engine/blob/master/packages/create-husky/README.md",
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "husky",
    "commitizen",
    "commitlint",
    "cz-customizable",
    "commitlint-config-cz",
    "lint-staged",
    "commitlint-config-cz",
    "cz",
    "工程化配置",
    "汉化cz工具",
    "angular-commit-standard"
  ]
}
