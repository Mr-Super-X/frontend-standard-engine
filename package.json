{
	"name": "frontend-standard-engine",
	"version": "1.0.0",
	"description": "自动生成前端规范配置的流程引擎",
	"author": "JP.Chen",
	"license": "MIT",
	"scripts": {
		"create:husky": "node ./node_modules/@frontend-standard-engine/create-husky/index.js",
		"prepare": "husky install",
		"commit": "git add . && cz",
		"push": "git add . && cz && git push"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.7",
		"@commitlint/config-conventional": "^17.6.7",
		"@mr.mikey/create-husky": "^1.0.7",
		"commitizen": "^4.3.0",
		"commitlint-config-cz": "^0.13.3",
		"cz-customizable": "^7.0.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.3"
	},
	"keywords": [],
	"repository": {
		"type": "git",
		"url": "https://github.com/Mr-Super-X/frontend-standard-engine.git"
	},
	"bugs": {
		"url": "https://github.com/Mr-Super-X/frontend-standard-engine/issues"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-customizable"
		},
		"cz-customizable": {
			"config": "./.cz-config.js"
		}
	}
}